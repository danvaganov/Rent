<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
	<application>
		<message-bundle>ru.bisoft.rent.jsf.bean.Messages_ru</message-bundle>
		<locale-config>
			<default-locale>ru</default-locale>
			<supported-locale>en_US</supported-locale>
			<supported-locale>en</supported-locale>
		</locale-config>
	</application>
	<converter>
		<display-name></display-name>
		<converter-id>contractStateConverter</converter-id>
		<converter-class>ru.bisoft.rent.jsf.converter.ContractStateConverter</converter-class>
	</converter>
	<managed-bean>
		<managed-bean-name>contractBean</managed-bean-name>
		<managed-bean-class>ru.bisoft.rent.jsf.bean.ContractBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>loginBean</property-name>
			<property-class>ru.bisoft.rent.jsf.bean.LoginBean</property-class>
			<value>#{loginBean}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>mainBean</managed-bean-name>
		<managed-bean-class>ru.bisoft.rent.jsf.bean.MainBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>loginBean</managed-bean-name>
		<managed-bean-class>ru.bisoft.rent.jsf.bean.LoginBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>articleBean</managed-bean-name>
