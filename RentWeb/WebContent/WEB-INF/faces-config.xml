			<property-name>loginBean</property-name>
			<property-class>ru.bisoft.rent.jsf.bean.LoginBean</property-class>
			<value>#{loginBean}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>articleCategoryBean</managed-bean-name>
		<managed-bean-class>ru.bisoft.rent.jsf.bean.ArticleCategoryBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>articleInStockBean</managed-bean-name>
