			<property-name>loginBean</property-name>
			<property-class>ru.bisoft.rent.jsf.bean.LoginBean</property-class>
			<value>#{loginBean}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>articleInRentBean</managed-bean-name>
