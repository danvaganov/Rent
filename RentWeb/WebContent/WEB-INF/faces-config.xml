		<managed-bean-class>ru.bisoft.rent.jsf.bean.ReportBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>loginBean</property-name>
			<property-class>ru.bisoft.rent.jsf.bean.LoginBean</property-class>
			<value>#{loginBean}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>articleWriteOffBean</managed-bean-name>
		<managed-bean-class>ru.bisoft.rent.jsf.bean.ArticleWriteOffBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>loginBean</property-name>
			<property-class>ru.bisoft.rent.jsf.bean.LoginBean</property-class>
			<value>#{loginBean}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>userBean</managed-bean-name>
		<managed-bean-class>ru.bisoft.rent.jsf.bean.UserBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

</faces-config>
