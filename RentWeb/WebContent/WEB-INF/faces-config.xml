		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>contractArticleBean</managed-bean-name>
		<managed-bean-class>ru.bisoft.rent.jsf.bean.ContractArticleBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
